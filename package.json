{
  "name": "qraga-woocommerce",
  "version": "0.2.0",
  "description": "Qraga - AI-Powered Shopping Assistant for WooCommerce",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:admin\" \"npm run dev:block\"",
    "dev:admin": "vite --config build-configs/vite.admin.config.ts",
    "dev:block": "wp-scripts start --config build-configs/webpack.block.config.cjs",
    "build": "npm run build:admin && npm run build:block",
    "build:admin": "tsc --project includes/admin/backend/tsconfig.json && vite build --config build-configs/vite.admin.config.ts",
    "build:block": "wp-scripts build --config build-configs/webpack.block.config.cjs",
    "lint": "npm run lint:admin && npm run lint:block",
    "lint:admin": "eslint includes/admin/backend/src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:block": "wp-scripts lint-js includes/admin/blocks/qraga-product-widget/src",
    "lint:css": "wp-scripts lint-style \"includes/admin/blocks/qraga-product-widget/src/**/*.scss\"",
    "format": "wp-scripts format includes/admin/blocks/qraga-product-widget/src",
    "watch": "concurrently \"npm run watch:admin\" \"npm run watch:block\"",
    "watch:admin": "tsc --project includes/admin/backend/tsconfig.json && vite build --config build-configs/vite.admin.config.ts --watch",
    "watch:block": "wp-scripts start --config build-configs/webpack.block.config.cjs",
    "package": "npm run build && npm run create-zip",
    "create-zip": "node scripts/create-zip.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/vite": "^4.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@wordpress/api-fetch": "^6.41.0",
    "@wordpress/block-editor": "^12.16.0",
    "@wordpress/blocks": "^12.25.0",
    "@wordpress/components": "^25.14.0",
    "@wordpress/element": "^5.25.0",
    "@wordpress/i18n": "^4.48.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.507.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.5.3",
    "react-select": "^5.7.7",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "yup": "^1.3.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.15",
    "@types/react-color": "^3.0.9",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@wordpress/scripts": "^27.6.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "next",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "wordpress",
    "woocommerce",
    "plugin",
    "qraga",
    "shopping-assistant"
  ],
  "author": "Qraga Team",
  "license": "GPL-3.0-or-later"
}
